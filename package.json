{
  "name": "react-ssr-example",
  "version": "1.0.0",
  "description": "Server side render for React with Express and SCSS",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/server.js --ext js,jsx --exec babel-node --presets @babel/env,@babel/react",
    "sass-watch": "sass --style=compressed --watch ./scss/index.scss ./public/css/styles.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reyarqueza/react-ssr-example.git"
  },
  "keywords": [
    "react",
    "ssr",
    "express"
  ],
  "author": "Rey Arqueza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reyarqueza/react-ssr-example/issues"
  },
  "homepage": "https://github.com/reyarqueza/react-ssr-example#readme",
  "dependencies": {
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "express": "^4.17.2",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
